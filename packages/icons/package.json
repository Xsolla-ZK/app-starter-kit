{
  "name": "@app/icons",
  "version": "0.0.1",
  "sideEffects": false,
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "yarn generate && tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "generate": "xzkui-icons generate -i ./raw-icons -o ./src/icons"
  },
  "dependencies": {
    "@tamagui/helpers-icon": "1.127.2"
  },
  "devDependencies": {
    "@app/typescript-config": "workspace:*",
    "@tamagui/build": "1.127.2",
    "@xsolla-zk/icons-generator": "latest",
    "react-native-svg": "15.8.0",
    "typescript": "5.8.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native": {
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./icons/Bow": {
      "import": "./dist/esm/icons/Bow.mjs",
      "require": "./dist/cjs/icons/Bow.cjs"
    },
    "./icons/Breastplate": {
      "import": "./dist/esm/icons/Breastplate.mjs",
      "require": "./dist/cjs/icons/Breastplate.cjs"
    },
    "./icons/Crossbow": {
      "import": "./dist/esm/icons/Crossbow.mjs",
      "require": "./dist/cjs/icons/Crossbow.cjs"
    },
    "./icons/Discord": {
      "import": "./dist/esm/icons/Discord.mjs",
      "require": "./dist/cjs/icons/Discord.cjs"
    },
    "./icons/KiteShield": {
      "import": "./dist/esm/icons/KiteShield.mjs",
      "require": "./dist/cjs/icons/KiteShield.cjs"
    },
    "./icons/LeatherHelm": {
      "import": "./dist/esm/icons/LeatherHelm.mjs",
      "require": "./dist/cjs/icons/LeatherHelm.cjs"
    },
    "./icons/Linkedin": {
      "import": "./dist/esm/icons/Linkedin.mjs",
      "require": "./dist/cjs/icons/Linkedin.cjs"
    },
    "./icons/LogoMonoLinkedin": {
      "import": "./dist/esm/icons/LogoMonoLinkedin.mjs",
      "require": "./dist/cjs/icons/LogoMonoLinkedin.cjs"
    },
    "./icons/Reddit": {
      "import": "./dist/esm/icons/Reddit.mjs",
      "require": "./dist/cjs/icons/Reddit.cjs"
    },
    "./icons/TowerShield": {
      "import": "./dist/esm/icons/TowerShield.mjs",
      "require": "./dist/cjs/icons/TowerShield.cjs"
    },
    "./icons/XTwitter": {
      "import": "./dist/esm/icons/XTwitter.mjs",
      "require": "./dist/cjs/icons/XTwitter.cjs"
    },
    "./icons/Youtube": {
      "import": "./dist/esm/icons/Youtube.mjs",
      "require": "./dist/cjs/icons/Youtube.cjs"
    }
  }
}